            <string>///Fire Bullets
if (not dead()) {
    if (is_state("firing")) {
        for (i = 0; i &lt; 1; i += 1) {
            if (floor(image_index) == image_number - 1 and boltsFired == i) {
                bDir = point_direction(x, bbox_bottom, targetX, targetY) + 90;
                with (instance_create(x, bbox_bottom, obj_FlyingSkull_bullet)) {
                    hSpeed = other.bSpeed * sin(other.bDir * pi / 180);
                    vSpeed = other.bSpeed * cos(other.bDir * pi / 180);
                    other.hSpeed = -hSpeed;
                    other.vSpeed = -vSpeed;
                    origin = other;
                }
                boltsFired += 1;
            }
        }
    }
    else {
        boltsFired = 0;
    }
    if (not (is_state("charging") or is_state("firing"))) {
        attacking = false;
    }
}
