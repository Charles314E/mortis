            <kind>1</kind>
            <string>///Float Towards Player
if (not dead()) {
    var theta = noone;
    var dist = -1;
    if (instance_exists(obj_Player)) {
        targetX = obj_Player.x;
        targetY = obj_Player.y;
        dist = point_distance(x, y, targetX, targetY);
        theta = point_direction(x, y, targetX, targetY) + 90;
    }
    else {
        targetX = x;
        targetY = y;
        dist = -1;
    }
    
    if (theta != noone) {
        inRadius = false;
        if (dist == max(0, min(dist, range))) {
            inRadius = true;
        }
        show_debug_message("[DBG]: " + tuple("dist", dist, "range", range));
        
        if (is_state("flying") and dist == max(0, min(dist, range * 2.5))) {
            if (not inRadius) {
                hSpeed = 1.5 * sin((theta * pi) / 180);
                vSpeed = 1.5 * cos((theta * pi) / 180);
            }
            hSpeed -= sign(hSpeed) * dSpeed * sin((theta * pi) / 180);
            vSpeed -= sign(vSpeed) * dSpeed * cos((theta * pi) / 180);
        }
    }
}
else {
    noGravity = false;
    hSpeed = 0;
}
</string>
